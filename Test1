local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Mining Champions") -- Creates the window
local b = w:CreateFolder("Center Inf Rocks") -- Creates the folder(U will put here your buttons,etc)

b:Toggle("Starter Inf Rock",function(bool)
    shared.toggle = bool
end)

spawn(function()
    while wait() do
        if shared.toggle then
           local oh1 = game:GetService("Workspace").Resources.Ores["World_1"]["Center_Ores"]["Ore_1"]
			game:GetService("ReplicatedStorage").Events.Server.RequestSwing:InvokeServer(oh1)
        end
    end
end)

b:Toggle("Grassland Inf Rock",function(bool1)
    shared.toggle1 = bool1
end)

